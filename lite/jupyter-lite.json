{
  "jupyter-lite-schema-version": 0,
  "jupyter-config-data": {
    "appName": "Jupyter Everywhere",
    "mode": "single-document",
    "contentsStorageName": "JupyterEverywhere Storage",
    "settingsStorageName": "JupyterEverywhere Storage",
    "exposeAppInBrowser": true,
    "showLoadingIndicator": true,
    "settingsOverrides": {
      "@jupyterlab/mainmenu-extension:plugin": {
        "menus": [
          {
            "disabled": true,
            "id": "jp-mainmenu-file"
          },
          {
            "disabled": true,
            "id": "jp-mainmenu-edit"
          },
          {
            "disabled": true,
            "id": "jp-mainmenu-view"
          },
          {
            "disabled": true,
            "id": "jp-mainmenu-run"
          },
          {
            "disabled": true,
            "id": "jp-mainmenu-kernel"
          },
          {
            "disabled": true,
            "id": "jp-mainmenu-tabs"
          },
          {
            "disabled": true,
            "id": "jp-mainmenu-settings"
          },
          {
            "disabled": true,
            "id": "jp-mainmenu-help"
          }
        ]
      },
      "@jupyterlab/application-extension:shell": {
        "startMode": "single"
      },
      "@jupyterlab/notebook-extension:tracker": {
        "autoStartDefaultKernel": true,
        "windowingMode": "none"
      },
      "@jupyterlab/application-extension:context-menu": {
        "disabled": true
      }
    },
    "disabledExtensions": [
      "@jupyterlab/extensionmanager-extension",
      "@jupyterlab/apputils-extension:announcements",
      "@jupyterlab/apputils-extension:state",
      "@jupyterlab/debugger-extension",
      "@jupyterlab/application-extension:property-inspector",

      "@jupyterlab/statusbar-extension",
      "@jupyterlab/notebook-extension:kernel-status",
      "@jupyterlab/apputils-extension:running-sessions-status",
      "@jupyterlab/apputils-extension:kernel-status",
      "@jupyterlab/running-extension:sidebar",

      "@jupyterlab/notebook-extension:toc",
      "@jupyterlab/toc-extension:registry",
      "@jupyterlab/toc-extension:tracker",

      "@jupyterlab/help-extension:jupyter-forum",

      "@jupyterlab/lsp-extension",
      "@jupyterlab/notebook-extension:language-server",
      "@jupyterlab/fileeditor-extension:language-server",
      "@jupyterlite/application-extension:lsp-connection-manager",

      "@jupyterlab/filebrowser-extension:widget",

      "@jupyterlab/notebook-extension:factory",

      "@jupyterlab/workspaces-extension",

      "@jupyterlab/console-extension",
      "@jupyterlab/tooltip-extension:consoles",
      "@jupyterlab/inspector-extension:consoles",
      "@jupyterlab/debugger-extension:consoles",

      "@jupyterlite/application-extension:about",
      "@jupyterlite/application-extension:logo",
      "@jupyterlite/application-extension:share-file"
    ]
  }
}
